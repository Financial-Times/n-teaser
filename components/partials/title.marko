<div class="o-teaser__heading js-teaser-heading">
	<a
		href="$!{relativeUrl}$!{referrerTracking}"
		class="js-teaser-heading-link"
		data-content-id="${id}"
		data-trackable="main-link"
		target=(type ==='promoted-content' && '_blank')>
		$!{nTeaserPresenter.displayTitle}
	</a>
	<span class="o-labels o-labels--premium" aria-label="Premium content" if(isPremium)>Premium</span>
</div>
